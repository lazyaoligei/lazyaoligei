.words{
  border: solid rgb(103, 10, 116);
  border-radius:10%;
  border-width:3px;
  width: 80%;
  margin-left: 32px;
  background: #e9d9fd;
  font-family: Microsoft JhengHei;
  font-size: 20px;
  height:30px;
  margin-top: 20px
}
.chue{
  text-align: center;
font-size: 30px;  
font-weight: bold;  
background: linear-gradient(to right, #ee102e, #8e0be6);  -webkit-background-clip: text;  
background-clip: text;  
color: transparent;
font-family: Microsoft JhengHei;
}
.yijian{
box-shadow:inset 0px 4px 1px -1px #efdcfb;
	background:linear-gradient(to bottom, #dfbdfa 5%, #bc80ea 100%);
	background-color:#dfbdfa;
	border-radius:15px;
	border:2px solid #8d3bc7;
	cursor:pointer;
	color:rgb(245, 82, 117);
	font-family:SimSun;
	font-size:25px;
	font-weight:bold;
	padding:9px 20px;
	text-decoration:none;
	text-shadow:1px 1px 11px #9752cc;
  margin-top: 10px;
  text-align: center;
}
.yijian:active {
	position:relative;
	top:1px;
}
.yijian:hover {
	background:linear-gradient(to bottom, #bc80ea 5%, #dfbdfa 100%);
	background-color:#bc80ea;
}
.nickname{
text-align: center;
font-size: 50px;  
font-weight: bold;  
background: linear-gradient(to right, #ee102e, #8e0be6);  -webkit-background-clip: text;  
background-clip: text;  
color: transparent;
font-family: Microsoft JhengHei;
}