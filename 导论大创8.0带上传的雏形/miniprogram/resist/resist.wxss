.weui-label {
  width: 130px;
  word-wrap: break-word;
  word-break: break-all;
  font-weight: bold;
  font-size: 20px;
  color: #b73ce7;
  font-family:Microsoft JhengHei;
}
.weui-cell {
  justify-content: flex-start;
  display: flex;
  align-items: center;
}
.all{
  width: 95%;
  border: solid #3b137c;
  margin-top: 20px;
  text-align: center;
  background: #d0e5fb;
  border-radius:5%;
  border-width:1px;
  box-shadow:6rpx 6rpx 6rpx 6rpx #620c92;
  margin-left: 5px;
  margin-bottom: 20px;
}
.head{
  font-weight: bold;
  font-size: 25px;
  color: #f58fad;
  font-family:Microsoft JhengHei;
}
.manorwoman{
margin-left: 70px;
}
.introduction{
  font-weight: bold;
  font-size: 23px;
  color: #b73ce7;
  font-family:Microsoft JhengHei;
  margin-top: 5px;
} 
.weui-input{
  border: solid rgb(245, 94, 152);
  border-radius:4%;
  border-width:1px;
  background: #e9d9fd;
  font-family: Microsoft JhengHei;
  height:25px;
  margin-top: 2px;
}
.weuiinput{
  border: solid rgb(245, 94, 152);
  border-radius:8%;
  border-width:2px;
  width: 90%;
  margin-left: 12px;
  background: #e9d9fd;
  font-family: Microsoft JhengHei;
  font-size: 18px;
  height:100px;
  margin-bottom: 10px;
}
.head2{
  font-weight: bold;
  font-size: 24px;
  color: #e21f59;
  font-family:SimSun;
  margin-top: 3px;
}
.tap {
	box-shadow:inset 0px 4px 1px -1px #efdcfb;
	background:linear-gradient(to bottom, #dfbdfa 5%, #bc80ea 100%);
	background-color:#dfbdfa;
	border-radius:15px;
	border:2px solid #8d3bc7;
	cursor:pointer;
	color:pink;
	font-family:Arial;
	font-size:15px;
	font-weight:bold;
	padding:9px 20px;
	text-decoration:none;
	text-shadow:1px 1px 11px #9752cc;
  margin-top: 10px;
  text-align: center;
  margin-bottom: 5px;
}
.tap:hover {
	background:linear-gradient(to bottom, #bc80ea 5%, #dfbdfa 100%);
	background-color:#bc80ea;
}
.tap:active {
	position:relative;
	top:1px;
}
